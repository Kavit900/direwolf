<h1>Search Job Titles: </h1>

<table>
  <thead>
    <tr>
      <th>Search</th>
      <th>Text field tag</th>
      <th colspan="3"></th>

    </tr>
  </thead>

  </table>
<%= form_tag search_results_path do %>
    <%= text_field_tag :keywords, placeholder="Enter title or tag..." %>
    <%= submit_tag 'Search' %>

   <h7> <%= render @jobs %> </h7>

    <% if !(session[:user_id].nil?) %>
        <a href="../jobseekers/">Back to Job Seeker Page</a>
    <% end %>
    <a href="../">Back to Homepage</a>
<% end %>